@using PassAppDev.Utils
@{
    ViewData["Title"] = "Home Page";
}

<h1>Customer List Of User In System</h1>
<form>
<input type="text" name="email" placeholder="Search by Category" />
<button asp-controller="Stores" type="submit" class="btn btn-primary">Search</button>
</form>
<table class="table">
	<thead>
		<tr>
			<th scope="col">Email</th>
			<th scope="col">Name</th>
			<th scope="col">Address</th>	
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<th scope="row">@item.UserName</th>
				<td>@item.FullName</td>
				<td>@item.Address</td>
			</tr>
		}

	</tbody>
</table>
